<%- include("headers") %>
    <div class="table-responsive">
        <h2>Solicitudes</h2>
        <table class="table table-striped table-sm">
            <thead>
                <tr>
                    <th scope="col">Solicitado</th>
                    <th scope="col">Estado</th>
                    <th scope="col">Fecha de inicio</th>
                    <th scope="col">Fecha final</th>
                </tr>
            </thead>
            <tbody id="myTable">
                <% collab.forEach(user=> { %>
                    <tr>
                        <td>
                            <%= user.vaca %>
                        </td>
                        <td>
                            <%= user.estado %>
                        </td>
                        <td>
                            <%= user.dateStart %>
                        </td>
                        <td>
                            <%= user.dateEnd %>
                        </td>
                    </tr>
                    <% }) %>
            </tbody>
        </table>
    </div>
    <%- include("footers") %>